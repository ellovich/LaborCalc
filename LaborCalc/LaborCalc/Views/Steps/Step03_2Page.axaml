<UserControl
    x:Class="LaborCalc.Views.Step03_2Page"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:laborCalc="clr-namespace:LaborCalc"
    xmlns:controls="clr-namespace:LaborCalc.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:models="clr-namespace:LaborCalc.Models"
    xmlns:viewModels="clr-namespace:LaborCalc.ViewModels"
    xmlns:views="clr-namespace:LaborCalc.Views"
    d:DesignHeight="960"
    d:DesignWidth="700"
    mc:Ignorable="d">

    <UserControl.Styles>
        <Style Selector="Border.step03style">
            <Setter Property="Background" Value="#303030" />
            <Setter Property="MaxWidth" Value="1100" />
            <Setter Property="MinWidth" Value="300" />
            <Setter Property="BorderBrush" Value="#303030" />
            <Setter Property="Border.BoxShadow" Value="0 0 1 1 #2000" />
            <Setter Property="Margin" Value="0, 0, 0, 0" />
            <Setter Property="Padding" Value="15, 0, 15, 10" />
            <Setter Property="BorderThickness" Value="0" />
            <Setter Property="CornerRadius" Value="10" />
        </Style>
    </UserControl.Styles>

    <DockPanel>

        <Grid
            MaxWidth="900"
            HorizontalAlignment="Left"
            ColumnDefinitions="*, *"
            DockPanel.Dock="Top">
            <StackPanel
                Grid.Column="0"
                Margin="10"
                VerticalAlignment="Center"
                Orientation="Horizontal">
                <TextBlock Text="Общий коэффициент новизны работы " />
                <TextBlock Text="k" />
                <TextBlock Classes="sub" Text="нов" />
                <TextBlock Text=" :" />
            </StackPanel>
            <ComboBox
                Grid.Column="1"
                VerticalAlignment="Center"
                Classes="correction"
                Items="{x:Static models:Step03_2.s_Corrections3_6}"
                SelectedItem="{Binding StepCorrection}">
                <ComboBox.ItemTemplate>
                    <DataTemplate>
                        <TextBlock Text="{Binding}" ToolTip.Tip="{Binding Coef}" />
                    </DataTemplate>
                </ComboBox.ItemTemplate>
            </ComboBox>
        </Grid>

        <TabControl DockPanel.Dock="Top">

            <!--  в части непотопляемости  -->
            <TabItem>
                <TabItem.Header>
                    <TextBlock>непотопляемость</TextBlock>
                </TabItem.Header>

                <StackPanel Spacing="15">

                    <CheckBox Margin="10" IsChecked="{Binding IsT_К}">
                        <StackPanel Orientation="Horizontal">
                            <TextBlock Classes="h" Text="T" />
                            <TextBlock Classes="hsub" Text="К " />
                            <TextBlock Classes="h" Text="(t₁-t₇)" />
                            <TextBlock Classes="h" Text=" - корабль/судно в целом" />
                        </StackPanel>
                    </CheckBox>

                    <!--  t1  -->
                    <Border Classes="step03style">

                        <Grid ColumnDefinitions="100, *, *, auto" RowDefinitions="auto, auto, auto, auto, auto, auto, auto, auto, auto">
                            <CheckBox
                                Name="CheckBox1"
                                Grid.Row="0"
                                Grid.Column="0"
                                Checked="ToggleButton_OnChecked"
                                IsChecked="{Binding IsT1}"
                                Unchecked="ToggleButton_OnUnchecked">
                                <TextBox Classes="h">t₁</TextBox>
                            </CheckBox>

                            <TextBox
                                Grid.Row="0"
                                Grid.Column="1"
                                Grid.ColumnSpan="3"
                                Margin="10"
                                Classes="h2"
                                Text="{x:Static models:Step03_2.T1}" />

                            <Border
                                Grid.Row="0"
                                Grid.Column="2"
                                Classes="labor">
                                <TextBlock Classes="labor" Text="{Binding _t1, FallbackValue='', StringFormat='{}{0} н/ч'}" />
                            </Border>

                            <StackPanel
                                Grid.Row="1"
                                Grid.Column="0"
                                Margin="10"
                                VerticalAlignment="Center"
                                Orientation="Horizontal">
                                <TextBlock Text="n" />
                                <TextBlock Classes="sub" Text="пш" />
                            </StackPanel>
                            <TextBlock
                                Grid.Row="1"
                                Grid.Column="1"
                                Margin="10"
                                Text="Кол-во шпангоутов корпуса корабля:"
                                TextWrapping="Wrap" />
                            <NumericUpDown
                                Grid.Row="1"
                                Grid.Column="2"
                                VerticalAlignment="Center"
                                Classes="count"
                                Value="{Binding N_пш}" />

                            <StackPanel
                                Grid.Row="2"
                                Grid.Column="0"
                                Margin="10"
                                VerticalAlignment="Center"
                                Orientation="Horizontal">
                                <TextBlock Text="k" />
                                <TextBlock Classes="sub" Text="1" />
                            </StackPanel>
                            <TextBlock
                                Grid.Row="2"
                                Grid.Column="1"
                                Margin="10"
                                Text="Степень сложности геометрии обводов корпуса:"
                                TextWrapping="Wrap" />
                            <ComboBox
                                Grid.Row="2"
                                Grid.Column="2"
                                VerticalAlignment="Center"
                                Classes="correction"
                                Items="{x:Static models:Step03_2.s_Corrections3_4_1}"
                                SelectedItem="{Binding K1}">
                                <ComboBox.ItemTemplate>
                                    <DataTemplate>
                                        <TextBlock Text="{Binding}" ToolTip.Tip="{Binding Coef}" />
                                    </DataTemplate>
                                </ComboBox.ItemTemplate>
                            </ComboBox>

                            <StackPanel
                                Grid.Row="3"
                                Grid.Column="0"
                                Margin="10"
                                VerticalAlignment="Center"
                                Orientation="Horizontal">
                                <TextBlock Text="k" />
                                <TextBlock Classes="sub" Text="нов" />
                            </StackPanel>
                            <TextBlock
